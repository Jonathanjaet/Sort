package javaapplication1;

public class Automoviles extends vehiculosTerrestres {

    //Campos o Variables de clase
    public static int capacidad;
    public static int kph;
    public static String tipoDeT;
    public static int precio;
    public static String color;
    public static String nombre;

    // Metodo constructor    
    public Automoviles(int precio, String color, String nombre, int capacidad, int kph, String tipoDeT) {
        this.precio = precio;
        this.color = color;
        this.nombre = nombre;
        this.capacidad = capacidad;
        this.kph = kph;
        this.tipoDeT = tipoDeT;

    }

    public Automoviles() {

    }
    public Automoviles(int precio,String color,String nombre,int capacidad,int kph) {
        
    }

    public Automoviles(String color) {
        this.color = color;
    }

    public String toString() {
        return ("El auto es " + nombre + " de color " + color
                + ", cuesta " + precio + ", es para " + capacidad + " pasajeros, corre hasta " + kph
                + " y tiene una transmisi√≥n" + tipoDeT);
    }
}
